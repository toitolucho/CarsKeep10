	<queryString>
		<![CDATA[select  am.IdActividad,  am.NombreActividad,  vsm.Costo, CASE vsm.CodigoEstadoEjecucion WHEN 'C' THEN 'CONCLUIDO' WHEN 'A' THEN 'ANULADO' WHEN 'P' THEN 'PENDIENTE' END AS EstadoEjecucion , vsm.Observacion
